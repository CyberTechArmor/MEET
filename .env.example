# MEET Environment Configuration
# Copy this file to .env and customize as needed

# Domain for the application (required for reverse proxy mode)
# Example: meet.example.com
MEET_DOMAIN=localhost

# LiveKit Configuration
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# URLs (auto-generated based on domain, but can be overridden)
# LIVEKIT_WS_URL=wss://meet.example.com:7880
# API_URL=https://meet.example.com/api
# CORS_ORIGIN=https://meet.example.com
